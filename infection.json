{
    "source": {
        "directories": [
            "src"
        ]
    },
    "timeout": 10,
    "logs": {
        "text": "infections.log",
        "stryker": {
            "report": "master"
        }
    },
    "mutators": {
        "@default": true,
        "global-ignoreSourceCodeByRegex": [
            "assert\\(.+\\);",
            "mkdir\\(\\$dir, 0777, true\\);"
        ]
    },
    "minMsi": 79,
    "minCoveredMsi": 80
}
