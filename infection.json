{
    "source": {
        "directories": [
            "src"
        ]
    },
    "timeout": 10,
    "logs": {
        "stryker": {
            "report": "/^\\d+\\.x$/"
        }
    },
    "mutators": {
        "@default": true,
        "global-ignoreSourceCodeByRegex": [
            "assert\\(.+\\);",
            "mkdir\\(\\$dir, 0777, true\\);"
        ]
    }
}
